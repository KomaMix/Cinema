@model IEnumerable<IdentityRole>

<h1>Roles</h1>

<p>
    <a asp-action="Create">Create New Role</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Name</td>
                <td>
                    <!-- Можно добавить действия для редактирования ролей -->
                </td>
            </tr>
        }
    </tbody>
</table>